{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-flow",
    ],
    "plugins": [
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-proposal-class-properties",
    ],
    "overrides": [
        {
            "test": "./",
            "presets": [
                ["@babel/preset-env", {
                    "targets": {
                        "browsers": ["last 2 versions"],
                    }
                }],
            ],
        },
    ],
}
